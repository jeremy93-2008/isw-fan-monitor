{
  "name": "isw-fan-monitor",
  "version": "1.0.0",
  "description": "ISW Fan Monitor is a fan monitor for the ISW.",
  "main": "main.js",
  "scripts": {
    "start": "npm run start:webpack",
    "start:webpack": "webpack --watch --config webpack.dev.config.js",
    "start:electron": "electron ./dist/main.js",
    "build:webpack": "webpack --config webpack.prod.config.js",
    "build": "npm run build:webpack && cp package.json dist/package.json && cp README.md dist/README.md && cp LICENSE dist/LICENSE && npx electron-packager ./dist",
    "package": "npx electron-installer-snap --src=./isw-fan-monitor-linux-x64 --confinement=strict"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Jeremy Auvray",
  "license": "MPL v2.0",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "copy-webpack-plugin": "^10.2.3",
    "css-loader": "^6.6.0",
    "electron": "^16.0.7",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "dependencies": {
    "@mh-cbon/sudo-fs": "^1.0.17",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3"
  }
}
